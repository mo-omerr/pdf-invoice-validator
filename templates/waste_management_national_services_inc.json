  "template_version": "1.0",
  "vendor": {
    "name": "Waste Management National Services, Inc.",
    "phone": "+1 888 625 5323",
    "address": "WM 1 Griffin Rd North Suite 4C, Windsor, CT, 06095",
    "email": "",
    "website": "www.wm.com/national"
  },
  "invoice_structure": {
    "header_fields": [
      "invoice_number",
      "date_of_issue",
      "due_date",
      "amount_due"
    ],
    "billing_fields": [
      "billed_to",
      "service_period",
      "gp_account_id"
    ],
    "line_item_fields": [
      "date",
      "material",
      "description",
      "size",
      "equipment_location_position",
      "po_frequency",
      "schedule",
      "sub_total",
      "tax",
      "total"
    ],
    "discount_types": [],
    "total_fields": [
      "subtotal",
      "tax",
      "total",
      "amount_due"
    ],
    "tax_info": {
      "tax_name": "State Sales/Use",
      "tax_rate": 0.06
    }
  },
  "validation_rules": {
    "required_fields": [
      "invoice_number",
      "date_of_issue",
      "due_date",
      "amount_due",
      "billed_to",
      "subtotal",
      "tax",
      "total"
    ],
    "date_format": "M/D/YYYY",
    "currency": "USD"
  },
  "extraction_hints": {
    "invoice_number_pattern": "\\d{10}",
    "date_pattern": "\\d{1,2}/\\d{1,2}/\\d{4}",
