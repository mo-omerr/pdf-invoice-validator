{
  "template_name": "CITY OF SEATTLE Invoice Template",
  "template_version": "1.0",
  "vendor": {
    "name": "CITY OF SEATTLE",
    "phone": "206-684-3000",
    "address": "PO BOX 35177, SEATTLE WA 98124-5177",
    "email": "SPUCustomerService@seattle.gov",
    "website": "myutilities.seattle.gov"
  },
  "invoice_structure": {
    "header_fields": [
      "invoice_number",
      "date_of_issue",
      "due_date",
      "amount_due"
    ],
    "billing_fields": [
      "billed_to",
      "account_number",
      "service_address"
    ],
    "line_item_fields": [
      "description",
      "service_period",
      "usage",
      "rate",
      "amount"
    ],
    "discount_types": [
      "adjustments",
      "credits"
    ],
    "total_fields": [
      "subtotal",
      "tax",
      "total",
      "amount_due"
    ],
    "tax_info": {
      "tax_name": "City and State Taxes",
      "tax_rate": 0.016
    }
  },
  "validation_rules": {
    "required_fields": [
      "invoice_number",
      "date_of_issue",
      "due_date",
      "amount_due",
      "billed_to",
      "subtotal",
      "tax",
      "total"
    ],
    "date_format": "M/D/YYYY",
    "currency": "USD"
  },
  "extraction_hints": {
    "invoice_number_pattern": "\\d{10}",
    "date_pattern": "\\d{1,2}/\\d{1,2}/\\d{4}",
    "amount_pattern": "\\$\\d+\\.\\d{2}",
    "multi_invoice_support": false,
    "pages_per_invoice": "4"
  }
}