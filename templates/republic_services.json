{
  "template_name": "Republic Services Invoice Template",
  "template_version": "1.0",
  "vendor": {
    "name": "Republic Services",
    "phone": "(704) 393-6900",
    "address": "4062 Section House Rd, Hickory NC 28601",
    "email": "",
    "website": "RepublicServices.com"
  },
  "invoice_structure": {
    "header_fields": [
      "invoice_number",
      "date_of_issue",
      "due_date",
      "amount_due"
    ],
    "billing_fields": [
      "billed_to",
      "account_number"
    ],
    "line_item_fields": [
      "description",
      "reference",
      "quantity",
      "unit_price",
      "amount"
    ],
    "discount_types": [
      "payments_adjustments"
    ],
    "total_fields": [
      "current_invoice_charges",
      "total",
      "amount_due"
    ],
    "tax_info": {
      "tax_name": "",
      "tax_rate": 0
    }
  },
  "validation_rules": {
    "required_fields": [
      "invoice_number",
      "date_of_issue",
      "due_date",
      "amount_due",
      "billed_to",
      "total"
    ],
    "date_format": "M/D/YYYY",
    "currency": "USD"
  },
  "extraction_hints": {
    "invoice_number_pattern": "\\d{4}-\\d{9}",
    "date_pattern": "\\d{1,2}/\\d{1,2}/\\d{4}",
    "amount_pattern": "\\$[\\d,]+\\.\\d{2}",
    "multi_invoice_support": true,
    "pages_per_invoice": "2"
  }
}